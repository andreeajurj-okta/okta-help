.coveo-custom-select-styled.active:after,
.coveo-custom-select-styled:active:after
{
	content: "";
	position: absolute;
	top: 45%;
	right: 15px;
	transform: translate(0, -50%) rotate(45deg);
	width: 9px;
	height: 9px;
	border: 1px #00297A;
	border-style: none solid solid none;
}

