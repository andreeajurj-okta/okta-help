	padding: 9px 16px 9px 0;
}

.coveo-custom-select-options li
{
	font-size: 16px;
	line-height: 1.4;
	text-indent: 0;
	padding: 8px 16px;
	border: none;
}

.coveo-custom-select-options li:hover,
.coveo-custom-select-options li.active
{
