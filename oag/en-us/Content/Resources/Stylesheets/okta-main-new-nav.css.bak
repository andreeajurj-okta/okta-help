.main-section
{
	padding-top: 140px;
}

#main-section
{
	padding-top: 70px;
	mc-conditions: 'MultiProdPubish.OIE, MultiProdPubish.Beta, MultiProdPubish.EndUser';
}

.main-section.hide-search
