	color: #C1C1C8;
}

.coveo-headline-wrapper .magic-box .magic-box-input .magic-box-underlay,
.coveo-headline-wrapper .magic-box .magic-box-input > input
{
	font-family: 'ABCWhyte', sans-serif;
	height: 38px;
