/* === In-topic tabs (see relnotes) ========================================================== */

.tabs
{
	border: 1px solid #C1C1C8;
	border-style: none none solid none;
	padding: 0;
	margin: 12px 0 32px;
	display: flex;
	flex-wrap: nowrap;
	overflow: auto;
}

.tabs:-webkit-scrollbar
{
	display: none;
}

ul.tabs li
{
	margin: 0;
}

.tabs-title a
{
	font-size: 16px;
	line-height: 1.4;
	color: #1D1D21;
	padding: 15px 24px;
	border-bottom: 3px solid transparent;
	white-space: nowrap;
}

.tabs-title > a:hover
{
	background: white;
	color: #1D1D21;
}

.tabs-title > a[aria-selected=true]
{
	background: white;
	color: #1D1D21;
	border-color: #1662DD;
}

.tabs-title > a:focus
{
	background: white;
	color: #1D1D21;
}

.tabs-content
{
	border: none;
}

/* override foundation setting--too much padding */

.tabs-panel
{
	padding: 0rem;
}

.tabs-panel.is-active
{
	display: block;
	outline: none;
}

