/* Replace Flare-generated top navigation with custom nav bar, representing support center nav: to allow left-accordian nav to function*/

div.title-bar-container,
#title-bar-container
{
	display: none;
}

div.replace_top_nav
{
	position: fixed;
	background-color: white;
	top: 0;
	left: 0;
	z-index: 99;
}

.navbar,
#navbar
{
	position: fixed;
	height: 70px;
	top: 0;
	left: 350px;
	right: 0;
	background-color: white;
	z-index: 99;
	display: flex;
	align-items: center;
	padding-left: 8px;
	padding-right: 64px;
}

.dropdown .dropbtn,
.dropdown a
{
	font-size: 16px;
	font-weight: 350;
	border: none;
	outline: none;
	padding: 24px 16px;
	margin: 0;
	height: 70px;
	display: inline-flex;
	align-items: center;
	color: #41414B;
}

.dropbtn i
{
	margin-left: 10px;
	margin-bottom: 8px;
	width: 9px;
	height: 9px;
	border: 1px #41414B;
	border-style: none solid solid none;
	transform: rotate(45deg);
}

.navbar a:hover,
.dropdown:hover .dropbtn
{
	color: #212126;
	background-color: transparent;
}

.dropdown-content
{
	display: none;
	position: absolute;
	background-color: white;
	width: auto;
	border: 1px solid #D7D7DC;
	z-index: 1;
	padding: 16px 0;
}

.dropdown-content a
{
	width: 100%;
	padding: 8px 24px;
	height: auto;
}

div.breadcrumbs
{
	margin: 0;
	padding: 0;
	position: absolute;
	top: 18px;
	left: 48px;
	font-size: 14px;
	color: #6E6E78;
	line-height: 1.2;
	z-index: 1;
}

div.breadcrumbs .MCBreadcrumbsLink
{
	color: #6E6E78;
	font-weight: 350;
	border-bottom: 1px solid #6E6E78;
}

div.breadcrumbs .MCBreadcrumbsDivider
{
	position: relative;
	color: transparent;
	margin: 0 6px;
	width: 12px;
	height: 12px;
}

div.breadcrumbs .MCBreadcrumbsDivider:after
{
	content: '';
	position: absolute;
	top: 50%;
	left: 2px;
	transform: translate(0, -50%) rotate(-45deg);
	width: 9px;
	height: 9px;
	border: 1px #41414B;
	border-style: none solid solid none;
}

.MCDropDown_Closed .MCDropDownHotSpot_.MCHotSpotImage
{
	background: none;
}

.MCDropDown_Closed .MCDropDownHotSpot_.MCHotSpotImage img,
.MCDropDown_Open .MCDropDownHotSpot_.MCHotSpotImage img
{
	display: none;
}

.MCDropDown_Open .MCDropDownHotSpot_.MCHotSpotImage
{
	background: none;
}

.MCDropDown_Open .MCDropDownHotSpot_.MCHotSpotImage:before,
.MCDropDown_Closed .MCDropDownHotSpot_.MCHotSpotImage:before
{
	content: '';
	display: inline-block;
	border: 5px solid transparent;
	border-left: 9px solid #6E6E78;
	margin-right: 5px;
	transform-origin: 5px 6px;
	transition: transform .1s;
}

.MCDropDown_Open .MCDropDownHotSpot_.MCHotSpotImage:before
{
	border-left: 9px solid #1662DD;
	transform: rotate(90deg);
}

/* End Replace Flare-generated top navigation*/
